{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "parent_id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "filters": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "field_name": {
            "type": "string"
          },
          "filter_type": {
            "type": "string"
          },
          "list_values": {
            "anyOf": [
              {
                "type": "null"
              },
              {
                "type": "object",
                "properties": {
                  "all": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "actual": {
                    "type": "null"
                  }
                },
                "required": [
                  "actual",
                  "all"
                ]
              }
            ]
          },
          "range_min_val": {
            "type": [
              "null",
              "string"
            ]
          },
          "range_max_val": {
            "type": [
              "null",
              "string"
            ]
          }
        },
        "required": [
          "field_name",
          "filter_type",
          "list_values",
          "name",
          "range_max_val",
          "range_min_val"
        ]
      }
    },
    "products": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "plu": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "image_links": {
            "type": "object",
            "properties": {
              "small": {
                "type": "array",
                "items": {
                  "format": "uri",
                  "type": "string"
                }
              },
              "normal": {
                "type": "array",
                "items": {
                  "format": "uri",
                  "type": "string"
                }
              }
            },
            "required": [
              "normal",
              "small"
            ]
          },
          "uom": {
            "type": "string"
          },
          "step": {
            "type": "string"
          },
          "rating": {
            "anyOf": [
              {
                "type": "null"
              },
              {
                "type": "object",
                "properties": {
                  "rating_average": {
                    "type": "number"
                  },
                  "rates_count": {
                    "type": "integer"
                  }
                },
                "required": [
                  "rates_count",
                  "rating_average"
                ]
              }
            ]
          },
          "promo": {
            "anyOf": [
              {
                "type": "null"
              },
              {
                "type": "object",
                "properties": {
                  "rebate": {
                    "type": "object",
                    "properties": {
                      "units_to_activate": {
                        "type": "integer"
                      }
                    },
                    "required": [
                      "units_to_activate"
                    ]
                  },
                  "n_plus_m": {
                    "type": "object",
                    "properties": {
                      "units_to_activate": {
                        "type": "integer"
                      },
                      "units_to_add": {
                        "type": "integer"
                      }
                    },
                    "required": [
                      "units_to_activate",
                      "units_to_add"
                    ]
                  }
                },
                "required": [
                ]
              }
            ]
          },
          "prices": {
            "type": "object",
            "properties": {
              "regular": {
                "type": "string"
              },
              "discount": {
                "type": [
                  "null",
                  "string"
                ]
              },
              "cpd_promo_price": {
                "type": [
                  "null",
                  "string"
                ]
              }
            },
            "required": [
              "cpd_promo_price",
              "discount",
              "regular"
            ]
          },
          "labels": {
            "anyOf": [
              {
                "type": "null"
              },
              {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "label": {
                      "type": "string"
                    },
                    "bg_color": {
                      "type": "string"
                    },
                    "text_color": {
                      "type": "string"
                    },
                    "icon_url": {
                      "type": "null"
                    }
                  },
                  "required": [
                    "bg_color",
                    "label",
                    "text_color"
                  ]
                }
              }
            ]
          },
          "property_clarification": {
            "type": "string"
          },
          "has_age_restriction": {
            "type": "boolean"
          },
          "stock_limit": {
            "type": "string"
          },
          "badges": {
            "type": "array"
          },
          "initial_weight_step": {
            "type": "string"
          },
          "min_weight": {
            "type": "string"
          },
          "orange_loyalty_points": {
            "type": [
              "integer",
              "null"
            ]
          },
          "is_available": {
            "type": "boolean"
          },
          "price_piece_unit": {
            "type": "null"
          }
        },
        "required": [
          "badges",
          "has_age_restriction",
          "image_links",
          "initial_weight_step",
          "is_available",
          "labels",
          "min_weight",
          "name",
          "orange_loyalty_points",
          "plu",
          "price_piece_unit",
          "prices",
          "promo",
          "property_clarification",
          "rating",
          "step",
          "stock_limit",
          "uom"
        ]
      }
    }
  },
  "required": [
    "filters",
    "name",
    "parent_id",
    "products"
  ]
}