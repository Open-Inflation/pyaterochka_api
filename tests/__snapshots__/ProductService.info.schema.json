{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "plu": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "image_links": {
      "type": "object",
      "properties": {
        "small": {
          "type": "array",
          "items": {
            "type": "string",
            "format": "uri"
          }
        },
        "normal": {
          "type": "array",
          "items": {
            "type": "string",
            "format": "uri"
          }
        }
      },
      "required": [
        "normal",
        "small"
      ]
    },
    "uom": {
      "type": "string"
    },
    "step": {
      "type": "string"
    },
    "rating": {
      "type": "object",
      "properties": {
        "rating_average": {
          "type": "number"
        },
        "rates_count": {
          "type": "integer"
        },
        "reviews_count": {
          "type": "null"
        }
      },
      "required": [
        "rates_count",
        "rating_average",
        "reviews_count"
      ]
    },
    "promo": {
      "type": "null"
    },
    "prices": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "value": {
            "type": "string"
          },
          "placement_type": {
            "type": "string"
          }
        },
        "required": [
          "placement_type",
          "value"
        ]
      }
    },
    "labels": {
      "type": "null"
    },
    "property_clarification": {
      "type": "string"
    },
    "has_age_restriction": {
      "type": "boolean"
    },
    "stock_limit": {
      "type": "string"
    },
    "initial_weight_step": {
      "type": "string"
    },
    "min_weight": {
      "type": "string"
    },
    "price_piece_unit": {
      "type": "null"
    },
    "description": {
      "type": "string"
    },
    "description_md": {
      "type": "string"
    },
    "description_html": {
      "type": "string"
    },
    "nutrients": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "value": {
            "type": "string"
          },
          "text": {
            "type": "string"
          }
        },
        "required": [
          "text",
          "value"
        ]
      }
    },
    "attributes": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "value": {
            "type": "string"
          },
          "uom": {
            "type": [
              "null",
              "string"
            ]
          }
        },
        "required": [
          "name",
          "uom",
          "value"
        ]
      }
    },
    "ingredients": {
      "type": "string"
    },
    "ingredients_html": {
      "type": "null"
    },
    "is_available": {
      "type": "boolean"
    },
    "is_various_manufacturers": {
      "type": "boolean"
    },
    "badges": {
      "type": "array"
    },
    "orange_loyalty_points": {
      "type": "integer"
    },
    "can_view_reviews": {
      "type": "boolean"
    }
  },
  "required": [
    "attributes",
    "badges",
    "can_view_reviews",
    "description",
    "description_html",
    "description_md",
    "has_age_restriction",
    "image_links",
    "ingredients",
    "ingredients_html",
    "initial_weight_step",
    "is_available",
    "is_various_manufacturers",
    "labels",
    "min_weight",
    "name",
    "nutrients",
    "orange_loyalty_points",
    "plu",
    "price_piece_unit",
    "prices",
    "promo",
    "property_clarification",
    "rating",
    "step",
    "stock_limit",
    "uom"
  ]
}